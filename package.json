{
  "name": "vite-vue",
  "version": "0.0.6",
  "license": "MIT",
  "scripts": {
    "prebuild": "rsw build",
    "build": "vite build",
    "check-types": "tsc --noemit --skipLibCheck",
    "dev": "vite",
    "watch": "rsw watch",
    "rsw": "rsw",
    "go:build": "make build",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "lint:tsc": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\" && npm run check-types",
    "start": "vite",
    "tauri": "tauri",
    "tauri:build": "npm run go:build && tauri build",
    "tauri:dev": "npm run go:build && tauri dev",
    "tauri:osxconfig": "cp ./src-tauri/.cargo/config_disabled ./src-tauri/.cargo/config"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.3",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "vite": "^2.9.1",
    "vite-plugin-rsw": "^2.0.2",
    "vue": "^3.2.32"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.8",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.32",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.6.0",
    "lint-staged": "^12.3.7",
    "rsw-node": "^1.0.2",
    "typescript": "^4.6.3",
    "wasm-pack": "^0.10.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
