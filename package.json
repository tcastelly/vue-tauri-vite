{
  "name": "vite-vue",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\" && npm run check-types",
    "check-types": "tsc --noemit --skipLibCheck",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "build": "vue-tsc --noEmit && vite build",
    "go:build": "cd ./src-tauri && make build",
    "tauri": "tauri",
    "tauri:dev": "npm run go:build && tauri dev",
    "tauri:build": "npm run go:build && tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.5",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "vite": "^2.4.4",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-beta.6",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.12.0",
    "lint-staged": "^11.0.0",
    "typescript": "^4.3.4",
    "vite-plugin-rsw": "^1.8.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
