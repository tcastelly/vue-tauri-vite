{
  "name": "vite-vue",
  "version": "0.0.3",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "prebuild": "rsw",
    "build": "vite build",
    "check-types": "tsc --noemit --skipLibCheck",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\" && npm run check-types",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "tauri:osxconfig": "cp ./src-tauri/.cargo/config_disabled ./src-tauri/.cargo/config",
    "go:build": "make build",
    "tauri": "tauri",
    "tauri:dev": "npm run go:build && tauri dev",
    "tauri:build": "npm run go:build && tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.8",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "vite": "^2.5.1",
    "vite-plugin-rsw": "^1.9.1",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^7.16.0",
    "lint-staged": "^11.1.2",
    "rsw-node": "^1.0.1",
    "typescript": "^4.3.5",
    "wasm-pack": "^0.10.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
