{
  "name": "vite-vue",
  "version": "0.0.5",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "prebuild": "rsw",
    "build": "vite build",
    "check-types": "tsc --noemit --skipLibCheck",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "eslint --quiet \"{tests,src}/**/*.{js,ts,tsx,vue}\" && npm run check-types",
    "lint:eslint": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
    "tauri:osxconfig": "cp ./src-tauri/.cargo/config_disabled ./src-tauri/.cargo/config",
    "go:build": "make build",
    "tauri": "tauri",
    "tauri:dev": "npm run go:build && tauri dev",
    "tauri:build": "npm run go:build && tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.8",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "vite": "^2.7.13",
    "vite-plugin-rsw": "^1.10.2",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.3.0",
    "lint-staged": "^12.3.1",
    "rsw-node": "^1.0.2",
    "typescript": "^4.5.5",
    "wasm-pack": "^0.10.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
